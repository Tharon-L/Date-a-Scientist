        <div class="controls">
            <button class="btn" onclick="previousSlide()">‚Üê Previous</button>
            <button class="btn" onclick="nextSlide()">Next ‚Üí</button>
            <button class="btn" onclick="toggleFullscreen()">Fullscreen</button>
